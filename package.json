{
  "name": "leadflow-ai",
  "version": "1.0.0",
  "description": "LeadFlow AI - Real Estate Investor Platform with PropStream & BatchSkipTrace Integration (2025)",
  "main": "index.js",
  "keywords": [
    "real-estate",
    "real-estate-crm",
    "property-investment",
    "propstream-integration",
    "skip-tracing",
    "batchskiptrace",
    "real-estate-leads",
    "property-analysis",
    "deal-evaluation",
    "real-estate-ai",
    "investor-platform",
    "wholesale-real-estate",
    "fix-and-flip",
    "buy-and-hold"
  ],
  "author": "Lead AI Team",
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "https://github.com/lead-ai/lead-ai-pro.git"
  },
  "homepage": "https://leadai.pro",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "dev:frontend": "next dev",
    "build": "npm run build:server && npm run build:frontend",
    "build:server": "tsc --project tsconfig.server.json",
    "build:frontend": "next build",
    "start": "node dist/server/index.js",
    "start:server": "ts-node server/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest tests/ai tests/lib --testPathIgnorePatterns=e2e",
    "test:integration": "jest tests/api --testPathIgnorePatterns=e2e",
    "test:e2e": "jest tests/e2e",
    "test:ai": "jest tests/ai",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint:check && npm run type-check && npm run test:ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@supabase/supabase-js": "^2.51.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^6.11.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
